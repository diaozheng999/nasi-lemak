{
  "name": "nasi-lemak",
  "version": "1.2.0-pre.11",
  "description": "A testable framework for React Native",
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "postbuild": "rollup -c rollup.config.js",
    "test": "jest",
    "lint": "tslint --project .",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/diaozheng999/nasi-lemak"
  },
  "module": "dist/esm/index.js",
  "author": "My M1 App Team",
  "license": "MIT",
  "dependencies": {
    "nasi": "^1.2.2",
    "nasi-lemak-end-point": "0.10.0-pre.2",
    "nasi-lemak-implementation": "1.2.0-pre.5",
    "nasi-lemak-react-types": "^1.1.1"
  },
  "devDependencies": {
    "rollup": "^1.24.0",
    "rollup-plugin-terser": "^5.2.0",
    "terser": "^4.3.9",
    "tslint": "^5.18.0",
    "tslint-no-focused-test": "^0.5.0",
    "typescript": "^3.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/dist"
    ],
    "globals": {
      "__DEV__": true
    }
  }
}
